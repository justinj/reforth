\ Need to tune this, I think it works, but it takes a ridiculous amount of time

: SMALLEST_PRIME_FACTOR ( n1 -- n2)
  DUP
  2 DO
    DUP I 
    MOD
    0= IF
      DROP
      I LEAVE
    THEN
  LOOP 
;

: REMOVE_SMALLEST_FACTOR ( n1 -- n2)
  DUP 
  SMALLEST_PRIME_FACTOR 
  /
;

: SOLVE
  DUP
  BEGIN
    SWAP
    DROP
    DUP REMOVE_SMALLEST_FACTOR
    DUP
  1 = UNTIL
  SWAP
;

100123 SOLVE .
bye
